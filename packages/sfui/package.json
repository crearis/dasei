{
  "name": "@crearis/sfui",
  "version": "0.0.9",
  "description": "a variant of @storefront-ui/vue with some customizations",
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js"
    },
    "./tailwind-config": {
      "types": "./dist/tailwind-config.d.ts",
      "import": "./dist/tailwind-config.mjs",
      "require": "./dist/tailwind-config.cjs.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "tailwind-config": [
        "dist/tailwind-config"
      ]
    }
  },
  "source": "index.ts",
  "scripts": {
    "build": "vite build",
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "postinstall": "pnpm run dev:prepare"
  },
  "dependencies": {
    "@crearis/tailwind-config": "workspace:*",
    "@floating-ui/vue": "^1.1.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@storefront-ui/shared": "^2.5.0",
    "@vueuse/core": "^10.9.0",
    "jw-paginate": "^1.0.4",
    "tabbable": "^6.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.0",
    "prettier": "^3.3.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vue-tsc": "^2.0.2",
    "@crearis/sfui-ts-config": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://docs.storefrontui.io/v2/#vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theaterpedia/crearis-ui.git",
    "directory": "packages/sfui/frameworks/vue"
  },
  "bugs": {
    "url": "https://github.com/theaterpedia/crearis-ui/issues"
  }
}